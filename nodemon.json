{
    "watch": [
        "*.js",
        "routes/",
        "controllers/",
        "models/",
        "config/"
    ],
    "ignore": [
        "node_modules/",
        "public/",
        "uploads/"
    ],
    "ext": "js,json",
    "delay": "1000",
    "env": {
        "NODE_ENV": "development"
    }
}